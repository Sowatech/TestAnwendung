@echo ==========================================================================
@echo Install npm packages
@echo ==========================================================================
call npm install
@echo ==========================================================================
@echo Install typings packages
@echo ==========================================================================
call typings install
@echo ==========================================================================
@echo Update webdriver-manager
@echo ==========================================================================
call .\node_modules\protractor\node_modules\.bin\webdriver-manager update
@echo ==========================================================================
@echo Install cordova/ionic packages/platforms
@echo ==========================================================================
call ionic cordova prepare
@echo ==========================================================================
@echo Ready
@echo ==========================================================================
pause